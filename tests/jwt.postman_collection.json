	"item": [
		{
			"name": "signin",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var url = pm.request.url.toString();\r",
							"var endpoint = url.split(\"localhost:4000\")[1];\r",
							"\r",
							"pm.test(endpoint + \" - Response status code is 200\", function () {\r",
							"  pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(endpoint + \" - Content-Type is application/json\", function () {\r",
							"    pm.expect(pm.response.headers.get(\"Content-Type\")).to.include(\"application/json\");\r",
							"});\r",
							"\r",
							"pm.test(endpoint + \" - Response time is less than 200ms\", function () {\r",
							"  pm.expect(pm.response.responseTime).to.be.below(200);\r",
							"});\r",
							"\r",
							"pm.test(endpoint + \" - Result object should have code, accessToken, and refreshToken fields\", function () {\r",
							"    const responseData = pm.response.json();\r",
							"    \r",
							"    pm.expect(responseData.result).to.exist.and.to.be.an('object');\r",
							"    pm.expect(responseData.result).to.have.property('code').that.is.a('number');\r",
							"    pm.expect(responseData.result).to.have.property('accessToken').that.is.a('string');\r",
							"    pm.expect(responseData.result).to.have.property('refreshToken').that.is.a('string');\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"auth": {
					"type": "basic"
				},
				"method": "POST",
				"header": [
					{
						"key": "Authorization",
						"value": "{{Authorization}}",
						"type": "text"
					}
				],
				"url": {
					"raw": "localhost:4000/signin",
					"host": [
						"localhost"
					],
					"port": "4000",
					"path": [
						"signin"
					]
				}
			},
			"response": []
